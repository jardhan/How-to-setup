#Create Voulume 

#Create stacks

version: "2.1"
services:
  heimdall:
    image: lscr.io/linuxserver/heimdall:latest
    container_name: heimdall
    environment:
      - PUID=1000
      - PGID=1000
      - TZ=Europe/London
    volumes:
      - </var/lib/docker/volumes/heimdall/_data>:/config #Volume path
    ports:
      - 82:80
      - 445:443
    restart: unless-stopped
